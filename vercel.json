{
  "functions": {
    "api/dramabox/detail/[bookId].js": {
      "memory": 128
    },
    "api/dramabox/allepisode/[bookId].js": {
      "memory": 128
    }
  },
  "rewrites": [
    {
      "source": "/api/dramabox/detail/:bookId",
      "destination": "/api/dramabox/detail/:bookId"
    },
    {
      "source": "/api/dramabox/allepisode/:bookId",
      "destination": "/api/dramabox/allepisode/:bookId"
    },
    {
      "source": "/api/:match*",
      "destination": "https://api.sansekai.my.id/api/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
